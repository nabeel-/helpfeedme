<div class="row">
  <%= render 'visitors/map_view' %>
</div>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3 search-area">
      <%= form_tag("/find", method: "get", class: 'form-inline') do %>
        <div class="form-group">
          <h2><%= label_tag(:q, "I'm in the mood for ") %></h2>
          <div class="row">
            <div class="col-sm-9">
              <%= text_field_tag :q, nil, class: 'form-control search-box' %>
            </div>
            <div class="col-sm-3">
              <%= submit_tag "Find it.", :name => nil, class: 'btn btn-med btn-orange search-btn' %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
